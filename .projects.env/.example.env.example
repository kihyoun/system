
PROJECT_NAME=example

PROD_HOST=www.example.com

PROD_PORT=80
PROD_DOMAIN_MODE=2
PROD_SSL=/etc/letsencrypt/live/www.example.com/fullchain.pem;
PROD_SSL_KEY=/etc/letsencrypt/live/www.example.com/privkey.pem;
PROD_UPSTREAM=prod

# Set false if not using a Beta Deployment
USE_BETA_HOST=true
BETA_HOST=beta.example.com
BETA_PORT=80
BETA_DOMAIN_MODE=2
BETA_SSL=/etc/letsencrypt/live/beta.example.com/fullchain.pem;
BETA_SSL_KEY=/etc/letsencrypt/live/beta.example.com/privkey.pem;
BETA_UPSTREAM=beta

# Set false if not using the Review
USE_REVIEW_HOST=true
REVIEW_HOST=example.com
REVIEW_PORT=80
REVIEW_DOMAIN_MODE=1
REVIEW_UPSTREAM=review